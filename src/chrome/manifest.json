{
    "name":"ZShort",
    "version":"1.0.1",
    "description":"Demo plugin",
	"manifest_version": 2,
	"browser_action": {
		"default_title": "Service shortlinks",
		"default_icon": { 
			"19": "img/icon19.png",
			"38": "img/icon38.png",
			"128" : "img/icon128.png"
		},
		"default_popup": "popup.html" 
	},
	"icons": {
		"128" : "img/icon128.png"
	},
	"permissions": [
		"tabs"
	]
}